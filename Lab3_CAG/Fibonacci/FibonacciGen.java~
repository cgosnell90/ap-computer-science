public class FibonacciGen
{
      public static void main (String args [])
         
    {
        FibonacciObject name = new FibonacciObject(100);
           
       
        while ( name.fibGenRe () > 0)
        {
          System.out.print (name.fibPrint ()+"\n");      
        }
            
      }
}